{
  "name": "flick-lifestyle",
  "version": "1.0.0",
  "description": "Flick Lifestyle E-commerce Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:admin\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:admin": "cd admin && npm run dev",
    "build": "npm run build:frontend && npm run build:admin",
    "build:frontend": "cd frontend && npm run build",
    "build:admin": "cd admin && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../admin && npm install",
    "start": "cd backend && npm start",
    "lint": "npm run lint:frontend && npm run lint:admin",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:admin": "cd admin && npm run lint",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules admin/node_modules",
    "clean:build": "rm -rf frontend/dist admin/dist",
    "test": "echo \"No tests specified\"",
    "postinstall": "echo \"Run 'npm run install:all' to install all dependencies\""
  },
  "keywords": [
    "ecommerce",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "express",
    "vite"
  ],
  "author": "Flick Lifestyle",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/flick-lifestyle.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/flick-lifestyle/issues"
  },
  "homepage": "https://github.com/your-username/flick-lifestyle#readme"
}
